<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>mulTable</servlet-name>
    <servlet-class>com.study.container.MulTable</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>mulTable</servlet-name>
    <url-pattern>/mul</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>stuServlet</servlet-name>
    <servlet-class>com.study.container.StuServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>stuServlet</servlet-name>
    <url-pattern>/stu</url-pattern>
  </servlet-mapping>
</web-app>
